alias Tuple<a, b> : { a : a, b : b }
type Tuple2<a, b> : { a : a, b : b }
alias Tuple3<a> : { a : a, b : a }

func main {
  ret int
} -> ret
  -> ret {
  let mut tup
  {
    tup.b = 42
    tup.a = 2
  }
  tup.a = 4
  tup.b = 3

  let mut tup2 : Tuple2<I64, Bool>
  tup2.a = 42
  tup2.b = true

  let tup3 : Tuple3<ret>
  let blub = tup == tup3

  ret = tup.b * 10 + tup.a
}
